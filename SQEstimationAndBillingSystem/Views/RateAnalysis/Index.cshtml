                return false;
                //alert('Item "'+item.data('langname')+' ('+item.data('lang')+')" selected by '+(e.type == 'keydown' ? 'pressing enter' : 'mouse click')+'.');
            }
        });
    });

    function calculateAll() {
        $("#txtContractorsProfitPer").trigger("change");
        CalculateLeadCharge(); 
        $("#txtAddForTribalAreaPer").trigger("change");
    }

    function CalculateAddForTribalAreaPer() {
        if ($("#txtTotal").val().trim() != '' && $("#txtAddForTribalAreaPer").val().trim() != '') {
            var AddForTribalAreaPer = ((parseFloat($("#txtTotal").val()) * parseFloat($("#txtAddForTribalAreaPer").val())) / 100);
            $("#txtAddForTribalArea").val(AddForTribalAreaPer);
            CalculateTotal();
        }
    }

    function CalculateContractorsProfitPer() {
        if ($("#txtCompletedRateForSSR").val().trim() != '' && $("#txtContractorsProfitPer").val().trim() != '') {
            var DeductForContractorsProfit = ((parseFloat($("#txtCompletedRateForSSR").val()) * parseFloat($("#txtContractorsProfitPer").val())) / 100);
            $("#txtDeductForContractorsProfit").val(DeductForContractorsProfit);
        }
    }
    

    function CalculateLeadCharge() {
        if ($("#txtBasicLead").val().trim() != '' && $("#txtConsumption").val().trim() != '') {
            $("#txtLeadCharge").val(parseFloat(parseFloat($("#txtBasicLead").val()) * parseFloat($("#txtConsumption").val())).toFixed(3));
        }
    }
    function CalculateTotal() {
        if ($("#txtCompletedRateForSSR").val().trim() != '' && $("#txtDeductForContractorsProfit").val().trim() != '') {
            //var DeductForContractorsProfit = ((parseFloat($("#txtCompletedRateForSSR").val()) * 20) / 100);
            //$("#txtDeductForContractorsProfit").val(DeductForContractorsProfit);
            var total = parseFloat($("#txtCompletedRateForSSR").val()) - parseFloat($("#txtDeductForContractorsProfit").val());
            $("#txtTotal").val(parseFloat(total).toFixed(3));
        }
    }

    function CalculateFinalCompletedRate() {
        if ($("#txtCompletedRateForSSR").val().trim() != '' && $("#txtAddForTribalArea").val().trim() != '') {
            var FinalCompletedRate = parseFloat($("#txtCompletedRateForSSR").val()) + parseFloat($("#txtTotalLeadCharges").val() != "" ? $("#txtTotalLeadCharges").val() : 0) + parseFloat($("#txtAddForTribalArea").val());
            $("#txtFinalCompletedRate").val(parseFloat(FinalCompletedRate).toFixed(3));
        }
    }
    function ClearForm() {
        $("#txtContractorsProfitPer").val('');
        $("#txtAddForTribalAreaPer").val('');

        $("#txtItemDescription").val('');
        $("#txtSSRItemNo").val('');
        $("#txtUnit").val('');
        $("#txtCompletedRateForSSR").val('');
        $("#txtDeductForContractorsProfit").val('');
        $("#txtTotal").val('');
        $("#txtMaterial").val('');
        $("#txtBasicLead").val('');
        $("#txtConsumption").val('');
        $("#txtLeadCharge").val('');
        $("#txtTotalLeadCharges").val('');
        $("#txtAddForTribalArea").val('');
        $("#txtFinalCompletedRate").val('');
        $("#id").val(0);
        $("#DSRDetailsId").val(0);
    }

    function ClearAll() {
        $("#txtUnit").val();
        $("#txtCompletedRateForSSR").val(0);
        $("#txtDeductForContractorsProfit").val();
        $("#txtTotal").val(0);
        $("#txtMaterial").val();
        $("#txtBasicLead").val(0);
        $("#txtConsumption").val(0);
        $("#txtLeadCharge").val(0);
        $("#txtTotalLeadCharges").val(0);
        $("#txtAddForTribalArea").val();
        $("#txtFinalCompletedRate").val(0);
        $("#id").val(0);
        $("#DSRDetailsId").val(0);
    }

