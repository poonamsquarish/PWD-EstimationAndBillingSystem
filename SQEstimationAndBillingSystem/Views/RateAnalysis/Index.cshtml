    function ValidateFields() {

        if ($("#txtItemDescription").val() == "" || $("#txtSSRItemNo").val() == "" || $("#txtUnit").val() == "" || $("#txtCompletedRateForSSR").val() == "") {
            toastr.error("Please enter mandatory fields(*).");
            return false;
        }
        return true;
    }

    function AddEditRateAnalysis(id) {

        $.ajax({
                type: 'GET',
            url: '@Url.Action("CreateEdit", "RateAnalysis")',
            data: { id: id },
            success: function (response) {
                debugger;
                $("#txtContractorsProfitPer").val(response.ContractorsProfitPer);

                $("#txtItemDescription").val(response.ItemDescription);
                $("#txtSSRItemNo").val(response.SSRItemNo);
                $("#txtUnit").val(response.Unit);
                $("#txtCompletedRateForSSR").val(response.CompletedRateForSSR);
                $("#txtDeductForContractorsProfit").val(response.DeductForContractorsProfit);
                $("#txtTotal").val(response.Total);
                $("#txtMaterial").val(response.Material);
                $("#txtBasicLead").val(response.BasicLead);
                $("#txtConsumption").val(response.Consumption);
                $("#txtLeadCharge").val(response.LeadCharge);
                $("#txtTotalLeadCharges").val(response.TotalLeadCharges);
                $("#txtAddForTribalAreaPer").val(response.AddForTribalAreaPer);
                $("#txtAddForTribalArea").val(response.AddForTribalArea);
                $("#txtFinalCompletedRate").val(response.FinalCompletedRate);
                $("#id").val(response.id);
                $("#DSRDetailsId").val(response.DSRDetailsId);
                    },
                error: function (result) {
                    toastr.error("Something went wrong. Please contact administrator.");
                }
            });

    }
    function DeleteRateAnalysis(id) {
        bootbox.confirm({
                message: "Are you sure?",
                buttons: {
                    confirm: {
                        label: "Yes",
                        className: 'btn-success'
                    },
                    cancel: {
                        label: "No",
                        className: 'btn-danger'
                    }
                }, callback: function (result) {
                    
                    if (!result)
                        return;
                    $.ajax({
                        type: 'POST',
                        url: '@Url.Action("DeleteRateAnalysis", "RateAnalysis")',
                        data: { id: id },
                        success: function (response) {
                            
                            if (response == "success") {
                                toastr.success("Deleted Rate Analysis Successfully.");
                                LoadGridData();
                              }
                            else {
                                toastr.error("Something went wrong. Please contact administrator.")
                                return;
                            }


                        },
                        error: function (result) {
                            toastr.error("Something went wrong. Please contact administrator.");
                        }
                    });
                }

            });
    }

   

