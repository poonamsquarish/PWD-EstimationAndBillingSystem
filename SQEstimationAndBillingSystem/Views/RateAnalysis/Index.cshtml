                            // toastr.error("Something went wrong while processing your request.");
                            console.log("GetSearchModalData");
                        }
                    });

                }
            },
            renderItem: function (item, search) {

                search = search.replace(/[-\/\\^$*+?.()|[\]{}]/g, '\\$&');
                var re = new RegExp("(" + search.split(' ').join('|') + ")", "gi");
                $('[data-toggle="tooltip"]').tooltip();
                return '<div class="autocomplete-suggestion" title="' + item[0] + '"   data-ItemOfWorkDesc="' + item[0] + '" data-ItemOfWorkId="' + item[1] + '" data-NetLeadChargesForCompletedCol="' + item[2] + '"  data-val="' + search + '">' + item[0].replace(re, "<b>$1</b>") + '</div>';
            },
            onSelect: function (e, term, item) {
                
                $("#txtMaterial").val(item.data('itemofworkdesc'));
               // $("#DSRDetailsId").val(item.data('itemofworkid'));

