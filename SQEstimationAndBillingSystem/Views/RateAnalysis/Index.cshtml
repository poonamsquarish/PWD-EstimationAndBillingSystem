<script>

    $(document).ready(function () {
      MakeReadOnlyIfNotLatestDSR("frmRateAnalysis");
        LoadGridData();
        $("#txtContractorsProfitPer").change(function () {
            if ($(this).val().trim() != '') {
                CalculateContractorsProfitPer();
                CalculateTotal();
            } else {
                $("#txtDeductForContractorsProfit").val('');
                $("#txtTotal").val('');
            }
        });
        $("#txtAddForTribalAreaPer").change(function () {
            if ($(this).val().trim() != '') {
                CalculateAddForTribalAreaPer();
                CalculateFinalCompletedRate();
            } else {
                $("#txtAddForTribalArea").val('');
                $("#txtFinalCompletedRate").val('');
            }
        });

        $("#txtConsumption").change(function () {
            if ($(this).val().trim() != '' && $("#txtBasicLead").val().trim() != '' ) {
                $("#txtLeadCharge").val(parseFloat(parseFloat($("#txtBasicLead").val()) * parseFloat($("#txtConsumption").val())).toFixed(2));
                $("#txtTotalLeadCharges").val(parseFloat($("#txtLeadCharge").val()).toFixed(2));
            } else {
                $("#txtLeadCharge").val('');
                $("#txtTotalLeadCharges").val('');
            }
        });

        $("#txtMaterial").on("keyup", function (e) {
            if ($(this).val().trim() == '') {
                $("#txtBasicLead").val('');
                $("#DSRDetailsId").val('0');
            }
        })
        

        $("#txtItemDescription").on("keyup", function (e) {
            if ($(this).val().trim() == '') {
                $("#Quantity").val('');
                $("#Quantity").removeAttr("readonly");
                $("#DSRDetailsId").val('0');
            }
        })


